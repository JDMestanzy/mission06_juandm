
@{
    ViewData["Title"] = "Delete Entry";
}
@model ApplicationResponse
<h2>Deleting Entry</h2>
<h3>Are you sure you want to delete the entry for "@Model.Title"?</h3>
<form method="post" asp-controller="Home" asp-action="Delete">
    <button type ="submit">Delete</button>
    <a asp-controller="Home" asp-action="ReadTable" class="btn btn-primary">Cancel</a>
    <input type ="hidden" asp-for="MovieID"/>
</form>